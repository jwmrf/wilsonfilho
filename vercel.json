{
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=0, must-revalidate"
          }
        ]
      }
    ],
    "routes": [
        { "src": "/en/(.*)", "dest": "/en/$1" },
        { "src": "/pt/(.*)", "dest": "/pt/$1" },
        { "src": "/(.*)", "dest": "/$1" }
      ]
  }
